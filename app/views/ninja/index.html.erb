      }

      .ui-tooltip, .arrow:after {
          background: white;
          border: 1px solid white;
      }
      .ui-tooltip {
          padding: 10px 20px;
          color: black;
          border-radius: 2px;
          font: bold 14px "Helvetica Neue", Sans-Serif;
          text-transform: uppercase;
          box-shadow: 0 0 1px black;
          border: 1px solid white !important;
      }
      .arrow {
          width: 70px;
          height: 16px;
          overflow: hidden;
          position: absolute;
          left: 50%;
          margin-left: -35px;
          bottom: -17px;
      }
      .arrow.top {
          top: -17px;
          bottom: auto;
      }
      .arrow.left {
          left: 20%;
      }
      .arrow:after {
          content: "";
          position: absolute;
          left: 20px;
          top: -20px;
          width: 25px;
          height: 25px;
          box-shadow: 0 0 1px black;
          -webkit-transform: rotate(45deg);
          -moz-transform: rotate(45deg);
          -ms-transform: rotate(45deg);
          -o-transform: rotate(45deg);
          transform: rotate(45deg);
      }
      .arrow.top:after {
          bottom: -20px;
          top: auto;
      }
      .hidden {
          visibility: hidden;
      }

      .fireTip {
          margin-left: -150px;
      }




  </style>
  <script>

      $( document ).ready(function() {
          $( document ).tooltip({
